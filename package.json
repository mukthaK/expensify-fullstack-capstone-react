{
    "name": "expensify-fullstack-capstone",
    "version": "1.0.0",
    "description": "An application to track the expenses.",
    "main": "js/index.js",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.18.3",
        "cheerio": "^1.0.0-rc.2",
        "chokidar-cli": "^1.2.1",
        "concurrently": "^4.0.1",
        "core-js": "^2.5.7",
        "cors": "^2.8.4",
        "cpr": "^3.0.1",
        "enzyme": "^3.6.0",
        "enzyme-react-16-adapter-setup": "^0.1.0",
        "express": "^4.16.3",
        "http-server": "^0.11.1",
        "jsonwebtoken": "^8.3.0",
        "moment": "^2.22.2",
        "mongodb": "^3.1.6",
        "mongoose": "^5.2.17",
        "morgan": "^1.9.1",
        "npm-run-all": "^4.1.3",
        "passport": "^0.4.0",
        "passport-http": "^0.3.0",
        "passport-jwt": "^4.0.0",
        "passport-jwt-strategy": "^1.1.5",
        "react": "^16.5.2",
        "react-addons-test-utils": "^15.6.2",
        "react-dom": "^16.5.2",
        "react-fontawesome": "^1.6.1",
        "react-redux": "^5.0.7",
        "redux": "^4.0.0",
        "redux-thunk": "^2.3.0",
        "rimraf": "^2.6.2",
        "unirest": "^0.5.1"
    },
    "devDependencies": {
        "acorn": "^6.0.1",
        "babel-cli": "^6.11.4",
        "babel-core": "^6.10.4",
        "babel-loader": "^6.2.4",
        "babel-plugin-transform-object-rest-spread": "^6.8.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "^6.13.1",
        "babel-preset-react": "^6.11.1",
        "chai": "^4.1.2",
        "chai-enzyme": "^1.0.0-beta.1",
        "chai-http": "^4.2.0",
        "enzyme-adapter-react-15": "^1.1.0",
        "enzyme-adapter-react-16": "^1.5.0",
        "faker": "^4.1.0",
        "json-loader": "^0.5.7",
        "mkdirp": "^0.5.1",
        "mocha": "^5.2.0",
        "react-scripts": "^1.1.5",
        "react-test-renderer": "^16.5.2",
        "sinon": "^6.3.4",
        "sinon-chai": "^3.2.0",
        "webpack": "^1.15.0",
        "webpack-cli": "^3.1.0"
    },
    "scripts": {
        "start": "http-server build",
        "dev": "run-p watch serve",
        "test": "mocha --recursive --compilers js:babel-core/register",
        "react-test": "react-scripts test --env=jsdom",
        "build": "run-s build:*",
        "clean": "rimraf build",
        "serve": "npm start",
        "watch": "run-p watch:*",
        "mkdir": "mkdirp build",
        "clean:html": "rimraf build/index.html",
        "mkdir:html": "run-s mkdir",
        "build:html": "run-s clean:html mkdir:html _build:html",
        "_build:html": "cp index.html build",
        "watch:html": "run-s build:html _watch:html",
        "_watch:html": "chokidar index.html -c \"npm run build:html\"",
        "clean:js": "rimraf build/js",
        "mkdir:js": "mkdirp build/js",
        "build:js": "run-s clean:js mkdir:js _build:js",
        "_build:js": "webpack",
        "watch:js": "run-s build:js _watch:js",
        "_watch:js": "webpack --watch",
        "mkdir:assets": "run-s mkdir",
        "clean:assets": "rimraf build/assets",
        "build:assets": "run-s clean:assets mkdir:assets _build:assets",
        "_build:assets": "cpr assets build/assets",
        "watch:assets": "run-s build:assets _watch:assets",
        "_watch:assets": "chokidar assets/**/* -c \"npm run build:assets\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mukthaK/expensify-fullstack-capstone-react.git"
    },
    "author": "Muktha Krishnaswamy",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/mukthaK/expensify-fullstack-capstone-react/issues"
    },
    "homepage": "https://github.com/mukthaK/expensify-fullstack-capstone-react#readme"
}
